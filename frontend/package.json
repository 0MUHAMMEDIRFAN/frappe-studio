{
	"name": "studio-ui",
	"private": true,
	"version": "0.0.0",
	"scripts": {
		"dev": "vite",
		"build": "vite build --base=/assets/studio/frontend/ && yarn copy-html-entry",
		"copy-html-entry": "cp ../studio/public/frontend/index.html ../studio/www/studio.html",
		"serve": "vite preview",
		"extract-frappeui-types": "tsx src/scripts/tsToJSONGenerator.ts frappeui",
		"extract-studio-types": "tsx src/scripts/tsToJSONGenerator.ts studio"
	},
	"dependencies": {
		"@vitejs/plugin-vue": "^4.2.3",
		"@vueuse/components": "^10.11.1",
		"@vueuse/core": "^10.11.1",
		"ace-builds": "^1.36.2",
		"autoprefixer": "^10.4.2",
		"feather-icons": "^4.28.0",
		"frappe-ui": "0.1.72",
		"lucide-vue-next": "^0.427.0",
		"pinia": "^2.2.1",
		"vite": "^4.4.9",
		"vue": "^3.4.12",
		"vue-router": "^4.2.2",
		"vuedraggable": "^4.1.0",
		"vue-sonner": "^1.3.0"
	},
	"devDependencies": {
		"eslint": "^8.39.0",
		"eslint-plugin-vue": "^9.11.0",
		"postcss": "^8.4.5",
		"prettier": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.6",
		"tailwindcss": "^3.4.3",
		"tsx": "^4.19.2",
		"typescript": "^5.5.4",
		"ts-json-schema-generator": "^2.3.0"
	}
}
