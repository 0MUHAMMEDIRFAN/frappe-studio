{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "DividerAction": {
      "type": "object",
      "properties": {
        "label": {
          "type": "string"
        },
        "handler": {
          "$comment": "() => any"
        },
        "loading": {
          "type": "boolean"
        }
      },
      "required": [
        "label",
        "handler"
      ],
      "additionalProperties": false
    },
    "DividerProps": {
      "type": "object",
      "properties": {
        "orientation": {
          "type": "string",
          "enum": [
            "horizontal",
            "vertical"
          ]
        },
        "position": {
          "type": "string",
          "enum": [
            "start",
            "center",
            "end"
          ]
        },
        "flexItem": {
          "type": "boolean"
        },
        "action": {
          "$ref": "#/definitions/DividerAction"
        }
      },
      "additionalProperties": false
    }
  }
}